{
  "name": "typed-chrome-webstore-api",
  "version": "0.3.7",
  "description": "Typed Chrome Webstore API to upload/publish extensions",
  "main": "dist/index.js",
  "engines": {
    "node": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cardinalby/typed-chrome-webstore-api"
  },
  "homepage": "https://github.com/cardinalby/typed-chrome-webstore-api",
  "keywords": [
    "chrome",
    "webstore",
    "api",
    "extension",
    "webextension",
    "upload",
    "publish",
    "download",
    "crx"
  ],
  "author": "cardinalby",
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "@types/node": "^11.11.3",
    "@types/axios": "^0.14.0",
    "prettier": "^1.16.3",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.4"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  }
}
